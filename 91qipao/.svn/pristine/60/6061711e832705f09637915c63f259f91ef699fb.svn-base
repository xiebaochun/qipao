function check_name_len() {
	var name = $('#nick-name').val();
	if (name.gblen() > 16)
		myToast("昵称最长8个汉字");
}

    $.fn.setCursorPosition = function(position){  
        if(this.lengh == 0) return this;  
        return $(this).setSelection(position, position);  
    }  
      
    $.fn.setSelection = function(selectionStart, selectionEnd) {  
        if(this.lengh == 0) return this;  
        input = this[0];  
      
        if (input.createTextRange) {  
            var range = input.createTextRange();  
            range.collapse(true);  
            range.moveEnd('character', selectionEnd);  
            range.moveStart('character', selectionStart);  
            range.select();  
        } else if (input.setSelectionRange) {  
            input.focus();  
            input.setSelectionRange(selectionStart, selectionEnd);  
        }  
      
        return this;  
    }  
      
    $.fn.focusEnd = function(){  
        this.setCursorPosition(this.val().length);  
    }  
    
$('#nick-name').focus(function(){
                $(this).attr("data-oval",$(this).val()); 
            }).blur(function(){
                var oldVal=($(this).attr("data-oval")); 
                var newVal=($(this).val());
                if (myNickname != newVal)
                {
					//checknick();               
                }
            });

$('.dialog-close-bt').click(function(){
				$('#authcode_frame').hide();			
             });

$('#get-authcode-bt').click(function(){
	var phone = $api.byId('phone').value;
    var info = {
			phone: phone,
			act: 3
			};
	api_ajax(0, 
		'util_authcode_get.php', 
		info, 
		after_send_Authcode,
		info,
		0
		);         	
	return false;
});                      
         
function after_send_Authcode(ret, _, _, info) {
	if (ret.code == 988) {
		return;
	}
	if (ret.code == 100) {
		$('#send_phone').html(info.phone);
		$('#authcode_frame').show();
    } else {
    	myToast(ret.msg);
    }
}
    
$('.valid-ensure-bt').click(function(){
	var phone = $api.byId('phone').value;
	var authcode = $api.byId('authcode').value;
	if (authcode == "") {myToast('验证码不能为空'); return;}
	var phone = $api.byId('phone').value;
    var info = {
			phone: phone,
			authcode: authcode
			};
	api_ajax(0, 
		'util_authcode_auth.php', 
		info, 
		after_Authcode_auth,
		info,
		0
		);         	
	return false;
});

function after_Authcode_auth(ret, _, _, info) {
	if (ret.code == 988) {
		return;
	}
	if (ret.code == 100){
		if (authcode == 1)
			authcode = 0;
		old_phone = info.phone;
		$('#authcode_frame').hide();
		$('#get-authcode-bt').hide();
		$('.fa-check-circle-o').show();	
		myToast('手机验证通过');	
	} else {
		myToast(ret.msg);
	}
}

       
function checknick() {
	var name = $('#nick-name').val();
	if (name.gblen() > 16) {
		myToast("昵称最长8个汉字");
		return false;
	};

	api_ajax(2, 
			'my_profile_set.php', 
			{	
				step:2,
				nickname: name
			}, 
			after_checknick,
			0,
			0
			); 
}
            
function after_checknick(ret, _, _, _) {
		if (ret.code == 988) {
			return;
		}
		if (ret.code == 100) {
			submit_profile();
		} else {
			myToast(ret.msg);
			//$('#nick-name').focusEnd();
		}
}            

function submit_profile() {
		var mobile = $('#phone').val().trim(); 
		if (mobile != '' && authcode > 0) {
			myToast("手机没有通过验证，请先验证手机。");
			return;
		}
		var userAvatorBase64 = "";
		if (avtarChange == 1)
        	userAvatorBase64 = $('#user-avator').data('baseData');

        var userGender = getGender();
        var nickName = $('#nick-name').val().trim();
 		if (nickName.gblen() == 0) {
			myToast("昵称不能为空");
			return;
		}
		       
 		var	bio = $('#resume').val();
		var city = 	$('#city').html();
		var birth =	$('#age').html();
		var	lookingfor = $('#sex-trend').html();
		     
		api_ajax(2, 
			'my_profile_set.php', 
			{	
				step: 1,
				nickname: nickName,
				sex: userGender,
				img: userAvatorBase64,
				bio: bio,
				city: city,
				birth: birth,
				lookingfor: lookingfor,
				mobile: mobile,
				platform:$api.getStorage('platform')
			}, 
			after_submit_profile,
			0,
			2
			);         
}

function after_submit_profile(ret, _, values, _) {
		if (ret.code == 988) {
			return;
		}
		if (ret.code == 100) {
		   $api.setStorage('set_my_change', 1);
		   avtarChange = 0;
		   $('#user-avator').data('baseData',ret.img);
		   $('#user-avator').css({'background-image':'url('+ret.img+')'});
		   $api.setStorage('username', ret.nickname);		   
		   $api.setStorage('userimage', ret.img);
		   if (values.mobile != "") {
		    	if (values.platform == 'qp_phone')
		   			$api.setStorage('phone', values.phone);
		   		else
		   			$api.setStorage('mobile', values.phone);
		   }
		   
			$('#nick-name').val(ret.nickname);
			myNickname = ret.nickname;		   
		   			
		    api.execScript({
		        name: 'root',
		        frameName: 'my',
		        script:  'view_appear();'
		    });
    		   			
    		if (ret.msg == '你一个月内只能修改一次昵称')
				myToast(ret.msg, 2000);
			else 
            	api.closeWin();
		} else {
			myToast(ret.msg, 2000);
		}
}

var authcode = 0;
function input_change(obj){
	var phone = $api.byId('phone').value;
	var myreg = /^(((13[0-9]{1})|(15[0-9]{1})|(18[0-9]{1}))+\d{8})$/; 
	if(myreg.test(phone)){ 
		$('#get-authcode-bt').show();
		//$('#authcode').attr('placeholder','验证码');
		if (phone != old_phone) {
			 $('.fa-check-circle-o').hide();
			 $('#get-authcode-bt').show();
			 authcode = 1;
		} else {
			 $('.fa-check-circle-o').show();
			 $('#get-authcode-bt').hide();
			 authcode = 0;
		}
	}
	else {
		$('#get-authcode-bt').hide();
	}
}
</script> 

